  <table class="table-bordered">
    <thead>
      <tr>
        <th>Project</th>
        <th>Week Start Date</th>
        <th>Week End Date</th>
        <th>Total Hours</th>
        <th>Details</th>
      </tr>
      
    </thead>
    <tbody>
      <% @time_sheet_details.each do |details| %>
        <tr>
          <td><%= details.project.try(:name) %></td>
          <td><%= details.start_date %></td>
          <td><%= details.end_date %></td>
          <td><%= details.hours %></td>
          <td><%= details.is_approved? ? "Approved" : "Submitted" %></td>
        </tr>
      <% end %>
    </tbody>
  </table>